{
  "name": "style-dictionary-storybook-style-guide",
  "version": "0.1.0",
  "description": "Example Style Dictionary style guide using Storybook",
  "scripts": {
    "styledictionary": "npm run build-styledictionary && chokidar 'src/**/*' 'tokens/**/*.json' sd.config.js -c 'npm run build-styledictionary'",
    "build-styledictionary": "style-dictionary build --config sd.config.js",
    "clean-styledictionary": "style-dictionary clean --config sd.config.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes",
    "test": "jest --coverage",
    "test-storybook": "test-storybook --coverage",
    "coverage": "cp coverage/storybook/coverage-storybook.json coverage/coverage-storybook.json && nyc report --reporter=html -t coverage --report-dir coverage"
  },
  "author": {
    "name": "John Barreiros",
    "email": "johnbarreiros2@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "@emotion/styled": "^11.10.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-dictionary": "^3.7.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-coverage": "^0.0.9",
    "@storybook/addon-essentials": "^7.0.5",
    "@storybook/react": "^7.0.5",
    "@storybook/react-webpack5": "^7.0.5",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/theming": "^7.0.5",
    "chokidar-cli": "^3.0.0",
    "chromatic": "^7.0.0",
    "jest": "^29.6.4",
    "jest-image-snapshot": "^6.2.0",
    "nyc": "^15.1.0",
    "storybook": "^7.0.5",
    "webpack": "^5.69.1"
  },
  "browserslist": [
    "defaults and supports es6-module"
  ]
}
